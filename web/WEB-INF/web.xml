        <servlet-name>PesquisaNivelcontroller</servlet-name>
        <servlet-class>controller.PesquisaNivelcontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaPedidoController</servlet-name>
        <servlet-class>controller.PesquisaPedidoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaProdutoController</servlet-name>
        <servlet-class>controller.PesquisaProdutoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PesquisaBairroController</servlet-name>
        <url-pattern>/PesquisaBairroController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaClienteController</servlet-name>
        <url-pattern>/PesquisaClienteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaFormaPagamentocontroller</servlet-name>
        <url-pattern>/PesquisaFormaPagamentocontroller</url-pattern>
