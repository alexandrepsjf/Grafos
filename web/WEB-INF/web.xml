        <servlet-name>PesquisaNivelcontroller</servlet-name>
        <url-pattern>/PesquisaNivelcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaPedidoController</servlet-name>
        <url-pattern>/PesquisaPedidoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaProdutoController</servlet-name>
        <url-pattern>/PesquisaProdutoController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
