    <servlet-mapping>
        <servlet-name>PesquisaLigacaocontroller</servlet-name>
        <url-pattern>/PesquisaLigacaocontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaUsuarioController</servlet-name>
        <url-pattern>/PesquisaUsuarioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaListaProduto</servlet-name>
        <url-pattern>/PesquisaListaProduto</url-pattern>
    </servlet-mapping>
